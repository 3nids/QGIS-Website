msgid "Transparency and blend mode."
msgstr ""

#: ../../source/site/forusers/visualchangelog260/index.rst:263
msgid "Item rotation."
msgstr ""

#: ../../source/site/forusers/visualchangelog260/index.rst:264
msgid "Map scale, extents and rotation."
msgstr ""

#: ../../source/site/forusers/visualchangelog260/index.rst:265
msgid "Atlas feature margin."
msgstr ""

#: ../../source/site/forusers/visualchangelog260/index.rst:266
msgid "Item size and position."
msgstr ""

#: ../../source/site/forusers/visualchangelog260/index.rst:267
